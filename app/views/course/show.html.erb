<p>
	<strong> First Name : </strong> 
	<%= @student.first_name %>
</p>

<p>
	<strong> Last Name : </strong>
	<%= @student.last_name %>
</p>

<p>
	<strong> Email : </strong>
	<%= @student.email %>
</p>

<p>
	<strong> Date_of_Birth : </strong>
	<%= @student.Date_of_Birth %>
</p>

<p>
	<strong> Section : </strong>
	<%= @student.section %>
</p>

